package hust.soict.sec.objectFb.user;

import com.restfb.DefaultFacebookClient;
import com.restfb.FacebookClient;
import com.restfb.Version;
import com.restfb.types.User;
import java.util.*;

public class UserInfo {

	String tokenUser = AccessToken.getAccessToken();
	
	public FacebookClient fbClient = new DefaultFacebookClient(tokenUser, Version.VERSION_12_0);
	User user = fbClient.fetchObject("me", User.class);
	
	public String userId = user.getId();
	public String fullname = user.getName();
	public UserInfo(String userId, String fullname) {
		super();
		this.userId = userId;
		this.fullname = fullname;
	}
	
	private ArrayList<UserInfo> fullUser = new ArrayList<UserInfo>();
}